    "construtoras": {
      "collection": "construtoras",
      "via": "user"
    },
    "nome": {
      "type": "string"
    },
    "sobrenome": {
      "type": "string"
    },
    "telefone": {
      "type": "string"
    },
    "funcao": {
      "type": "string"
    },
    "tipo_morador": {
      "type": "boolean"
    },
    "cliente": {
      "type": "boolean"
    },
    "admin": {
      "type": "boolean"
    },
    "ativo": {
      "type": "boolean",
